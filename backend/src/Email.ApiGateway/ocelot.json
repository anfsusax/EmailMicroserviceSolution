{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/emails",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "email-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/emails",
      "UpstreamHttpMethod": [ "POST" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/emails/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "email-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/emails/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/auth/token",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "email-api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/auth/token",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:9000",
    "RequestIdKey": "OcRequestId",
    "DownstreamScheme": "http"
  }
}

